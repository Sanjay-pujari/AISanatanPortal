{
  "ConnectionStrings": {
    "DefaultConnection": "Host=YOUR_PRODUCTION_HOST;Database=AISanatanPortalDB;Username=YOUR_DB_USER;Password=YOUR_DB_PASSWORD;SSL Mode=Require"
  },
  "JwtSettings": {
    "SecretKey": "YOUR_PRODUCTION_SECRET_KEY_MUST_BE_VERY_SECURE",
    "Issuer": "AISanatanPortal",
    "Audience": "AISanatanPortalUsers",
    "ExpirationHours": 24
  },
  "AzureOpenAI": {
    "Endpoint": "YOUR_AZURE_OPENAI_ENDPOINT",
    "ApiKey": "YOUR_AZURE_OPENAI_API_KEY",
    "DeploymentName": "gpt-4o",
    "ApiVersion": "2024-05-01-preview"
  },
  "GoogleMaps": {
    "ApiKey": "YOUR_GOOGLE_MAPS_API_KEY"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Error",
      "AISanatanPortal": "Information"
    }
  },
  "AllowedHosts": "aisanatanportal.com,www.aisanatanportal.com",
  "EmailSettings": {
    "SmtpServer": "smtp.gmail.com",
    "SmtpPort": 587,
    "SenderName": "AI Sanatan Portal",
    "SenderEmail": "noreply@aisanatanportal.com",
    "Username": "YOUR_EMAIL_USERNAME",
    "Password": "YOUR_EMAIL_PASSWORD"
  },
  "ApplicationSettings": {
    "ApplicationName": "AI Sanatan Portal",
    "Version": "1.0.0",
    "Environment": "Production",
    "DefaultPageSize": 20,
    "MaxPageSize": 100
  }
}